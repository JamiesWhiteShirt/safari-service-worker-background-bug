{
  "manifest_version": 3,
  "name": "Web extension",
  "version": "1.0.0",
  "description": "Web extension demonstrating a cross origin request bug with background service workers.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_title": "Click to make a request"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://localhost:3000/*"]
}
